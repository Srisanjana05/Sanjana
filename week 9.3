def differenceSum(n):
    n_str = str(n)
    even_sum = 0
    odd_sum = 0
    for i, digit in enumerate(n_str):
        digit = int(digit)
        if i % 2 == 0:
            
            even_sum += digit
        else:
            
            odd_sum += digit
    
    difference = abs(even_sum - odd_sum)
    return difference
